{
  "name": "some-sum-type",
  "version": "0.0.1",
  "description": "Flexible sum type / ADT library powered by sanctuary-def",
  "author": {
    "name": "Kurt Milam",
    "email": "kurt.milam@xioup.com",
    "url": "http://xioup.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xioup/sum-type-validation.git"
  },
  "keywords": [
    "sum type",
    "union type",
    "adts",
    "algabraic data types",
    "functional programming",
    "sanctuary",
    "sanctuary-def",
    "ramda",
    "static",
    "validation"
  ],
  "main": "src/index.js",
  "scripts": {
    "bundle": "sh -c \"cp node_modules/babel-polyfill/dist/polyfill.min.js dist/index.js && NODE_ENV=production rollup -f umd --name \"createSumTypeFactory\" -c -i src/index.js >> dist/index.js\"",
    "lint": "eslint .",
    "migrate": "node ./scripts/migrate",
    "test": "sh -c \"npm run lint && NO_USERAGENT=1 mocha --require ./test-helper.js\"",
    "testAsync": "sh -c \"npm run lint && NO_USERAGENT=1 mocha --require ./test-helper.js -t 4000\""
  },
  "dependencies": {
    "ramda": "0.24.x",
    "sanctuary": "0.13.x",
    "sanctuary-def": "0.13.x",
    "sanctuary-type-identifiers": "2.0.x"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "6.22.x",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^4.1.1",
    "eslint-plugin-babel": "^4.1.2",
    "istanbul": "0.4.x",
    "mocha": "^3.4.2",
    "rollup": "^0.43.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "engines": {
    "node": "6.x"
  }
}
